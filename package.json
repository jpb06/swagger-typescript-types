{
  "name": "swagger-typescript-types",
  "description": "Generating typescript artifacts from a remote swagger json",
  "version": "3.0.8",
  "main": "index.js",
  "repository": "https://github.com/jpb06/swagger-typescript-types.git",
  "license": "MIT",
  "bin": {
    "generateTypesFromUrl": "./dist/cli/generate-types-from-url.cli.js",
    "generateTypesFromFile": "./dist/cli/generate-types-from-file.cli.js"
  },
  "dependencies": {
    "@apidevtools/openapi-schemas": "^2.1.0",
    "axios": "^1.4.0",
    "chalk": "4.1.2",
    "fs-extra": "^11.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@swc/core": "^1.3.56",
    "@swc/jest": "^0.2.26",
    "@ts-morph/bootstrap": "^0.19.0",
    "@ts-morph/common": "^0.19.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-mock": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^2.8.8",
    "readme-package-icons": "^1.1.8",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "copy-package": "copyfiles package.json ./dist/",
    "copy-readme": "copyfiles README.md ./dist/",
    "build": "del-cli ./dist && tsc --project tsconfig.prod.json",
    "postbuild": "pnpm copy-package && pnpm copy-readme",
    "test": "jest",
    "test-changes": "jest --watch",
    "test-dev": "jest --coverage --watchAll",
    "test-silentcov": "jest --coverage --silent --runInBand --watchAll=false",
    "test-ci": "jest --ci --coverage",
    "lint": "eslint ./src",
    "type-check": "tsc --noEmit",
    "cli": "node ./dist/cli/generate-types-from-file.cli.js",
    "sync-icons": "pnpm generateReadmeIcons -h 50"
  }
}
